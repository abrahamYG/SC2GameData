        TriggerExecute(gt_ActivateLeftBridge, true, false);
        TriggerExecute(gt_EndCorruptorPatrol, true, false);
        libNtve_gf_ShowHideUnit(UnitFromId(3274), true);
    }

    if ((gv_cinematicCompleted == false)) {
